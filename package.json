{
  "name": "date-picker-svelte",
  "version": "2.3.0",
  "description": "Date and time picker for Svelte",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-package && vite build",
    "build:package": "svelte-package",
    "build:site": "vite build",
    "preview": "vite preview",
    "lint": "prettier --check src *.js && eslint src *.js && svelte-check",
    "format": "prettier --write src *.js && eslint --fix src *.js",
    "test": "jest tests",
    "test:coverage": "jest --collect-coverage --collectCoverageFrom=src/lib/**"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.6",
    "@sveltejs/kit": "^1.15.9",
    "@sveltejs/package": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-jest": "^29.5.0",
    "date-fns": "^2.29.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^29.5.0",
    "mdsvex": "^0.10.6",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "sass": "^1.62.1",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  },
  "peerDependencies": {
    "svelte": "^3.24.0"
  },
  "license": "MIT",
  "homepage": "https://date-picker-svelte.kasper.space",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/probablykasper/date-picker-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/probablykasper/date-picker-svelte/issues"
  },
  "author": {
    "name": "Kasper Henningsen",
    "url": "https://kasper.space"
  },
  "keywords": [
    "date",
    "time",
    "date-picker",
    "datepicker",
    "date-time-picker",
    "calendar",
    "svelte",
    "front-end"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "pluginSearchDirs": [
      "."
    ]
  }
}
